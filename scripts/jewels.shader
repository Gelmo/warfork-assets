textures/jewels/neon_red
{
	qer_editorimage textures/jewels/neon_red.png
	q3map_lightimage textures/jewels/neon_blue.png
	surfaceparm nomarks
	surfaceparm nonsolid
	surfaceparm noimpact
	surfaceparm trans
	surfaceparm nolightmap
	nopicmip

	{
		map textures/jewels/neon_alpha.png
		rgbGen teamcolor 2
		blendfunc add
	}
}
