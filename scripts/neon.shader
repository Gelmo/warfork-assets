textures/neon/neon_square
{
    surfaceparm nomarks
    surfaceparm nonsolid
    qer_editorimage textures/neon/neon_square.png
    q3map_lightimage textures/neon/neon_square.png
    q3map_surfacelight 50
    {
        map textures/neon/neon_square.png
        blendfunc add
    }
}

