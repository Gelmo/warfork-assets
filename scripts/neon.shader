textures/neon/basskick
{
	qer_editorimage textures/neon/basskick1.png
	q3map_lightimage   textures/neon/basskick1.png
	q3map_surfacelight 500
	q3map_forceMeta
	q3map_lightmapSampleSize 64
	q3map_lightsubdivide 64
	surfaceparm	nomarks
	surfaceparm trans
	surfaceparm nonsolid
	nopicmip
	{
		animmap 1 textures/neon/basskick1.png textures/neon/basskick2.png textures/neon/basskick3.png textures/neon/basskick4.png
	}
}

